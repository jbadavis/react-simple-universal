{
  "name": "react-universal",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "dev": "npm start",
    "clean": "rm -r ./dist || true",
    "start": "webpack-dev-server --hot --inline --config webpack/webpack.config.dev.js",
    "build:client": "webpack -p --config webpack/webpack.config.prod.js",
    "build:server": "babel src -d dist --ignore Client.js",
    "start:server": "node dist/Server.js",
    "build:prod": "NODE_ENV=prod && npm run clean && npm run build:client && npm run build:server",
    "prod": "npm run build:prod && npm run start:server"
  },
  "author": "jbadavis",
  "license": "ISC",
  "repository": "jbadavis/react-universal",
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.27.3",
    "ejs": "^2.5.6",
    "eslint": "^3.19.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.1",
    "nunjucks": "^3.0.1",
    "nunjucks-webpack-plugin": "^3.0.0",
    "react": "^16",
    "react-dom": "^16",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.2",
    "webpack-merge": "^4.1.0"
  }
}
